{
  "name": "Filesystem Access Example",
  "version": "1",
  "manifest_version": 2,
  "icons": {
    "16": "camera.png",
    "128": "camera.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "experimental",
    "fileSystem",
    "fileSystem.write"
  ],
  "intents": {
    "http://webintents.org/view" : [{
      "title" : "Txt Reader",
      "type" : ["text/*"],
      "disposition" : "window"
    }],
    "http://webintents.org/edit" : [{
      "title" : "Txt Reader",
      "type" : ["text/*"],
      "disposition" : "window"
    }]
  }
}
